    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Populate Script",
            "type": "python",
            "request": "launch",
            "program": "populate.py",
            "args": ["--file", "~/Desktop/show_urls.csv"],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Populate Script with Backfill",
            "type": "python",
            "request": "launch",
            "program": "populate.py",
            "args": ["--file", "~/Desktop/show_urls.csv", "--backfill"],
            "console": "integratedTerminal"
        }
    ]
}
